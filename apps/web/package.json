{
  "name": "@atlas/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format --write .",
    "db:types": "bash -c 'PROJECT_ID=$(echo $NEXT_PUBLIC_SUPABASE_URL | sed -E \"s|https://([^.]+)\\.supabase\\.co.*|\\1|\") && supabase gen types typescript --project-id \"$PROJECT_ID\" --schema public > src/lib/supabase/database.types.ts'"
  },
  "dependencies": {
    "@atlas/core": "workspace:*",
    "@supabase/supabase-js": "2.75.0",
    "next": "15.5.9",
    "openai": "4.77.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@atlas/tsconfig": "workspace:*",
    "@supabase/ssr": "^0.7.0"
  }
}
